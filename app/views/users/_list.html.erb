<h1>User info</h1>
  <!-- 追加部分 -->
<div><%= image_tag user.get_profile_image(100,100) %></div>
<h4>name <%= user.name %></h4>
<h4>introduction <%= user.introduction %></h4>

<!-- form_with部分 -->
<%= form_with model: Book.new do |f| %>
  <h1>New book</h1>
  <p>Title</p>
  <%= f.text_field :title %>
  <p>Opinion</p>
  <%= f.text_area :body %>
  <%= f.submit 'CreateBook' %>
<% end %>